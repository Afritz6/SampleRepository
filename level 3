#include <iostream>
#include <string>
using namespace std;

class Person
{
protected:
	string p_address;
	string p_name;
	int p_phoneNumber;
	string address;
	int phoneNumber;
	string GetName()
	{
		return p_name;
	}
	void SetName(const string& name)
	{
		p_name = name;
	}

	string getAddress()
	{
		return p_address;
	}
	void SetAddress(const string& name)
	{
		p_address = address;
	}
	int getPhoneNumber()
	{
		return p_phoneNumber;
	}
	void SetPhoneNumber(const string& name)
	{
		p_phoneNumber = phoneNumber;
	}
	
};
class Customer:public Person
{
	int transaction;
	float credit;

	float Credit()
	{

		return credit;
	}
	int Transactions()
	{

		return transaction;
	}
};
class Employee: public Person
{
	float wage;
	int months;
	string job;
	void underlings()
	{
		
	}

	float setwage()
	{
		return wage;
	}
	int MonthsWorked()
	{
		return months;
	}
	string Job()
	{
		return job;
	}
};



